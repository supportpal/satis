                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/chronicle.zip"
                },
                "require": {
                    "ext-pdo": "*",
                    "guzzlehttp/guzzle": "^6",
                    "monolog/monolog": "^1.17",
                    "paragonie/blakechain": "^1",
                    "paragonie/easydb": "^2",
                    "paragonie/sapient": "^1",
                    "paragonie/slim-sapient": "^1",
                    "php": "^7",
                    "roave/security-advisories": "dev-master",
                    "slim/php-view": "^2.0",
                    "slim/slim": "^3.8",
                    "tflori/getopt-php": "^2"
                },
                "require-dev": {
                    "phpunit/phpunit": "^6",
                    "vimeo/psalm": "^0.3"
                },
                "type": "project",
                "autoload": {
                    "classmap": [
                        "src/routes.php",
                        "src/Chronicle/Chronicle.php",
                        "src/Chronicle/HandlerInterface.php",
                        "src/Chronicle/MiddlewareInterface.php",
                        "src/Chronicle/Scheduled.php",
