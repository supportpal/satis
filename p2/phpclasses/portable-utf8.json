{
    "packages": {
        "phpclasses/portable-utf8": [
            {
                "name": "phpclasses/portable-utf8",
                "version": "3.0.106",
                "version_normalized": "3.0.106.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/portable-utf8.zip"
                },
                "require": {
                    "php": ">=7.0.0",
                    "symfony/polyfill-iconv": "~1.0",
                    "symfony/polyfill-intl-grapheme": "~1.0",
                    "symfony/polyfill-intl-normalizer": "~1.0",
                    "symfony/polyfill-mbstring": "~1.0",
                    "symfony/polyfill-php72": "~1.0",
                    "voku/portable-ascii": "~1.4"
                },
                "require-dev": {
                    "phpunit/phpunit": "~6.0 || ~7.0"
                },
                "type": "library",
                "autoload": {
                    "psr-4": {
                        "voku\\": "src/voku/",
                        "voku\\tests\\": "tests/"
                    },
                    "files": [
                        "bootstrap.php"
                    ]
                }
            }
        ]
    }
}
