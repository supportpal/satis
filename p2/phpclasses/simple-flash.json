{
    "packages": {
        "phpclasses/simple-flash": [
            {
                "name": "phpclasses/simple-flash",
                "version": "1.2.4",
                "version_normalized": "1.2.4.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/simple-flash.zip"
                },
                "require": {
                    "php": ">=5.4.0"
                },
                "require-dev": {
                    "phpunit/phpunit": "4.*",
                    "scrutinizer/ocular": "1.*"
                },
                "type": "library",
                "extra": {
                    "branch-alias": {
                        "dev-master": "2.0.x-dev",
                        "1.2.x": "1.2.x-dev",
                        "1.1.x": "1.1.x-dev",
                        "1.0.x": "1.0.x-dev"
                    }
                },
                "autoload": {
                    "psr-4": {
                        "Tamtamchik\\SimpleFlash\\": "src"
                    },
                    "files": [
                        "src/function.php"
                    ]
                }
            }
        ]
    }
}
