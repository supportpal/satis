{
    "packages": {
        "phpclasses/lumen-starter-pack": [
            {
                "name": "phpclasses/lumen-starter-pack",
                "version": "1.0.1",
                "version_normalized": "1.0.1.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/lumen-starter-pack.zip"
                },
                "require": {
                    "guzzlehttp/guzzle": "^6.2",
                    "laravel/lumen-framework": "5.1.*",
                    "lucadegasperi/oauth2-server-laravel": "4.0.x",
                    "optimus/oauth2-server-lumen": "0.1.*",
                    "vlucas/phpdotenv": "~1.0"
                },
                "require-dev": {
                    "phpunit/phpunit": "~4.0"
                },
                "type": "project",
                "autoload": {
                    "psr-4": {
                        "App\\": "app/"
                    },
                    "classmap": [
                        "database/"
                    ],
                    "files": [
                        "app/Support/helpers.php"
                    ]
                }
            }
        ]
    }
}
