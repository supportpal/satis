{
    "packages": {
        "phpclasses/phpcodefixer": [
            {
                "name": "phpclasses/phpcodefixer",
                "version": "1.0.24",
                "version_normalized": "1.0.24.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/phpcodefixer.zip"
                },
                "require": {
                    "ext-json": "*",
                    "ext-tokenizer": "*",
                    "php": ">=5.4",
                    "symfony/console": "^3.4"
                },
                "type": "package",
                "extra": {
                    "phar-builder": {
                        "compression": "BZip2",
                        "name": "phpcf-dev.phar",
                        "output-dir": "./",
                        "entry-point": "bin/phpcf",
                        "include": [
                            "bin",
                            "data"
                        ],
                        "events": {
                            "command.package.start": "git describe --tags > bin/version.txt",
                            "command.package.end": "cp phpcf-dev.phar phpcf-`cat bin/version.txt`.phar && chmod +x phpcf.phar && rm bin/version.txt"
                        }
                    }
                },
                "autoload": {
                    "psr-4": {
                        "wapmorgan\\PhpCodeFixer\\": "src/"
                    },
                    "files": [
                        "src/functions.php"
                    ]
                }
            }
        ]
    }
}
