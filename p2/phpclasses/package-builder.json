{
    "packages": {
        "phpclasses/package-builder": [
            {
                "name": "phpclasses/package-builder",
                "version": "1.4",
                "version_normalized": "1.4.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/package-builder.zip"
                },
                "require": {
                    "symfony/console": "2.6.7",
                    "symfony/filesystem": "2.6.7",
                    "symfony/process": "2.6.7",
                    "wapmorgan/php-rpm-packager": "0.1.5",
                    "wdalmut/php-deb-packager": "0.0.11"
                },
                "require-dev": {
                    "phpunit/phpunit": "4.6"
                },
                "type": "library",
                "autoload": {
                    "psr-4": {
                        "wapmorgan\\PackageBuilder\\": "src"
                    }
                }
            }
        ]
    }
}
