{
    "packages": {
        "phpclasses/phpca": [
            {
                "name": "phpclasses/phpca",
                "version": "1.12",
                "version_normalized": "1.12.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/phpca.zip"
                },
                "require": {
                    "docopt/docopt": "~1.0"
                },
                "type": "package",
                "extra": {
                    "phar-builder": {
                        "compression": "BZip2",
                        "name": "phpca.phar",
                        "output-dir": "./",
                        "entry-point": "bin/phpca",
                        "include": [
                            "bin",
                            "data"
                        ],
                        "events": {
                            "command.package.start": "git describe --tags > bin/version.txt",
                            "command.package.end": "chmod +x phpca.phar"
                        }
                    }
                },
                "autoload": {
                    "psr-4": {
                        "wapmorgan\\PhpCodeAnalyzer\\": "src/"
                    }
                }
            }
        ]
    }
}
