{
    "packages": {
        "phpclasses/ci-recaptcha": [
            {
                "name": "phpclasses/ci-recaptcha",
                "version": "1.0.0",
                "version_normalized": "1.0.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/ci-recaptcha.zip"
                },
                "require": {
                    "afelicioni/codeigniter-installer-plugin": ">=0.2.0",
                    "php": ">=5.3.0"
                },
                "type": "afelicioni-codeigniter",
                "extra": {
                    "target-path": ".",
                    "setup-files": [
                        [
                            "config/Recaptcha.php",
                            "application/config/Recaptcha.php"
                        ],
                        [
                            "libraries/Recaptcha.php",
                            "application/libraries/Recaptcha.php"
                        ],
                        [
                            "controllers/Recaptcha.php",
                            "application/controllers/Recaptcha.php"
                        ]
                    ]
                },
                "autoload": {
                    "classmap": [
                        "controllers/Recaptcha.php",
                        "libraries/Recaptcha.php"
                    ]
                }
            }
        ]
    }
}
