{
    "packages": {
        "phpclasses/laravel-currency": [
            {
                "name": "phpclasses/laravel-currency",
                "version": "1.0.0",
                "version_normalized": "1.0.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/laravel-currency.zip"
                },
                "require": {
                    "laravel/framework": ">=6.0",
                    "php": ">=7.2",
                    "raggitech/laravel-readable": "^1.0"
                },
                "type": "library",
                "extra": {
                    "laravel": {
                        "providers": [
                            "RaggiTech\\Laravel\\Currency\\CurrencyServiceProvider"
                        ]
                    }
                },
                "autoload": {
                    "psr-4": {
                        "RaggiTech\\Laravel\\Currency\\": "src"
                    },
                    "files": [
                        "src/Helpers.php"
                    ]
                }
            }
        ]
    }
}
