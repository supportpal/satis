{
    "packages": {
        "phpclasses/event-mediator": [
            {
                "name": "phpclasses/event-mediator",
                "version": "2.0.1",
                "version_normalized": "2.0.1.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/event-mediator.zip"
                },
                "require": {
                    "ext-spl": "*",
                    "php": ">=7.0, <7.2",
                    "pimple/pimple": "~3.0"
                },
                "require-dev": {
                    "henrikbjorn/phpspec-code-coverage": "dev-master",
                    "nilportugues/php_backslasher": "^1.1",
                    "phpspec/nyan-formatters": "~2.0.0",
                    "phpspec/phpspec": "~3.1.0",
                    "phpunit/php-code-coverage": "~4.0",
                    "symfony/console": "~2.8"
                },
                "type": "library",
                "extra": {
                    "branch-alias": {
                        "dev-master": "2.*-dev"
                    }
                },
                "autoload": {
                    "psr-0": {
                        "Spec\\": "specs/"
                    },
                    "psr-4": {
                        "EventMediator\\": [
                            "src/"
                        ]
                    }
                }
            }
        ]
    }
}
