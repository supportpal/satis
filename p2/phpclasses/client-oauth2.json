{
    "packages": {
        "phpclasses/client-oauth2": [
            {
                "name": "phpclasses/client-oauth2",
                "version": "1.0.0",
                "version_normalized": "1.0.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/client-oauth2.zip"
                },
                "require": {
                    "poirot/api-client": "dev-devel",
                    "poirot/ioc": "dev-devel",
                    "poirot/std": "dev-devel",
                    "psr/http-message": "1.0"
                },
                "type": "library",
                "autoload": {
                    "psr-4": {
                        "Poirot\\OAuth2Client\\": "src/",
                        "Module\\OAuth2Client\\": "mod/"
