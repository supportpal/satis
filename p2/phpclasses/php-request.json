{
    "packages": {
        "phpclasses/php-request": [
            {
                "name": "phpclasses/php-request",
                "version": "1.1.5",
                "version_normalized": "1.1.5.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/php-request.zip"
                },
                "require": {
                    "josantonius/validate": "^1.0",
                    "php": "^7.0"
                },
                "require-dev": {
                    "friendsofphp/php-cs-fixer": "^2.3 || ^2.8",
                    "phpmd/phpmd": "^2.6",
                    "phpunit/phpunit": "^5.7 || ^6.0",
                    "squizlabs/php_codesniffer": "^3.0"
                },
                "type": "library",
                "extra": {
                    "branch-alias": {
                        "dev-master": "2.0-dev"
                    }
                },
                "autoload": {
                    "psr-4": {
                        "Josantonius\\Request\\": "src/"
                    }
                }
            }
        ]
    }
}
