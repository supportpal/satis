{
    "packages": {
        "phpclasses/php-logger-lib": [
            {
                "name": "phpclasses/php-logger-lib",
                "version": "1.1.3",
                "version_normalized": "1.1.3.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/php-logger-lib.zip"
                },
                "require": {
                    "josantonius/json": "^1.1.5",
                    "php": "^5.6 || ^7.0"
                },
                "require-dev": {
                    "phpunit/phpunit": "5.7.*",
                    "squizlabs/php_codesniffer": "3.*"
                },
                "type": "library",
                "extra": {
                    "branch-alias": {
                        "dev-master": "1.0-dev"
                    }
                },
                "autoload": {
                    "psr-4": {
                        "Josantonius\\Logger\\": "src/"
                    }
                }
            }
        ]
    }
}
