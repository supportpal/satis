{
    "packages": {
        "phpclasses/laravel-sms": [
            {
                "name": "phpclasses/laravel-sms",
                "version": "1.0.0",
                "version_normalized": "1.0.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/laravel-sms.zip"
                },
                "require": {
                    "guzzlehttp/guzzle": "^6.0",
                    "php": "^7.0|~7.2"
                },
                "require-dev": {
                    "orchestra/testbench": "^3.8",
                    "phpunit/phpunit": "^7.0"
                },
                "type": "library",
                "extra": {
                    "laravel": {
                        "providers": [
                            "Djunehor\\Sms\\SmsServiceProvider"
                        ]
                    }
                },
                "autoload": {
                    "psr-4": {
                        "Djunehor\\Sms\\": "src/"
                    },
                    "files": [
                        "src/Helpers/helpers.php"
                    ]
                }
            }
        ]
    }
}
