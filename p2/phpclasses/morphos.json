{
    "packages": {
        "phpclasses/morphos": [
            {
                "name": "phpclasses/morphos",
                "version": "1.88",
                "version_normalized": "1.88.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/morphos.zip"
                },
                "require": {
                    "ext-mbstring": "*",
                    "php": ">=5.4.0"
                },
                "require-dev": {
                    "phpunit/phpunit": "~4.8"
                },
                "type": "library",
                "autoload": {
                    "files": [
                        "src/English/functions.php",
                        "src/Russian/functions.php"
                    ],
                    "psr-4": {
                        "morphos\\": "src/"
                    }
                }
            }
        ]
    }
}
