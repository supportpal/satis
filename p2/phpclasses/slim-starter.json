{
    "packages": {
        "phpclasses/slim-starter": [
            {
                "name": "phpclasses/slim-starter",
                "version": "1.0.1",
                "version_normalized": "1.0.1.0",
                "dist": {
                    "type": "zip",
                    "url": "https://www.phpclasses.org/install/package/slim-starter.zip"
                },
                "require": {
                    "cartalyst/sentry": "2.*",
                    "illuminate/database": "4.*",
                    "ircmaxell/password-compat": "1.0.*",
                    "itsgoingd/slim-facades": "dev-master",
                    "php": ">= 5.3.0",
                    "slim/slim": "2.4.*",
                    "slim/views": "0.1.2",
                    "twig/extensions": "*",
                    "twig/twig": "1.*"
                },
                "type": "library",
                "autoload": {
                    "psr-4": {
                        "_empty_": [
                            "src/",
                            "app/controllers/",
                            "app/models/",
                            "app/middlewares/",
                            "app/modules/"
                        ]
                    }
                }
            }
        ]
    }
}
